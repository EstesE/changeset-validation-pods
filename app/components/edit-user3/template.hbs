
<div class="col-md-6">
    <div><label>First Name</label></div>
    {{input type="text" value=changeset.firstName}} model value: {{model.firstName}}
</div>

<div class="col-md-6">
    <div><label>Last Name</label></div>
    {{input type="text" value=changeset.lastName}} model value: {{model.lastName}}
</div>

<div class="col-md-6">
    <div><label>Country</label></div>
    {{input type="text" value=changeset.address$country}} model value: {{model.address.country}}
</div>

<div class="col-md-6">
    <div><label>Town</label></div>
    {{input type="text" value=changeset.address$town}} model value: {{model.address.town}}
</div>

<div class="col-md-6">
    <div><label>State Name</label></div>
    {{input type="text" value=changeset.address$state$name}} model value: {{model.address.state.name}}
</div>

<div class="col-md-12" style="margin-top: 25px;">
    <button type="button" class="btn btn-primary" {{action back changeset}}>{{backText}}</button>
    <button type="button" class="btn btn-default" {{action save changeset}}>{{saveText}}</button>
</div>

